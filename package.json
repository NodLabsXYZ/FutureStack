{
  "name": "basic-next-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "supabase": "yarn && supabase stop && supabase start && npx prisma migrate deploy",
    "stop": "supabase stop",
    "dev": "npx next dev",
    "dev:next_template": "NEXT_TEMPLATE=1 npx next dev --port 3001",
    "build": "npx next build",
    "start": "npx next start",
    "lint": "npx next lint",
    "test": "npx jest --watch",
    "test:next_template": "npx jest --testLocationInResults --forceExit --run-in-band --reporters=\"<rootDir>/__tests__/next_template_reporter.js\"",
    "test:next_template:path": "sh -c 'npx jest ${0} --testLocationInResults --forceExit --runTestsByPath --run-in-band --reporters=\"<rootDir>/__tests__/next_template_reporter.js\"'",
    "test:ci": "npx jest --runInBand",
    "e2e": "npx playwright test __e2e__",
    "upgrade-all": "yarn yarn-upgrade-all"
  },
  "dependencies": {
    "@bundlr-network/client": "^0.7.1",
    "@headlessui/react": "^1.5.0",
    "@heroicons/react": "^1.0.6",
    "@prisma/client": "^3.12.0",
    "@ramonak/react-progress-bar": "^4.4.0",
    "@stripe/react-stripe-js": "^1.7.1",
    "@stripe/stripe-js": "^1.27.0",
    "@supabase/supabase-auth-helpers": "^1.3.0",
    "@supabase/supabase-js": "^1.33.3",
    "@tailwindcss/typography": "^0.5.2",
    "@walletconnect/web3-provider": "^1.7.7",
    "arweave": "^1.10.23",
    "arweave-nft-uploader": "^1.1.2",
    "autoprefixer": "^10.4.4",
    "ethers": "^5.6.2",
    "formidable": "^2.0.1",
    "next": "^12.1.5",
    "postcss": "^8.4.12",
    "react": "^18.0.0",
    "react-code-blocks": "^0.0.9-0",
    "react-currency-format": "^1.1.0",
    "react-dom": "^18.0.0",
    "react-ellipsis-text": "^1.2.1",
    "react-json-view": "^1.21.3",
    "react-markdown": "^8.0.2",
    "react-spinners": "^0.11.0",
    "react-test-renderer": "^18.0.0",
    "request": "^2.88.2",
    "sharp": "^0.30.3",
    "store2": "^2.13.2",
    "stripe": "^8.216.0",
    "survey-react": "^1.9.24",
    "tailwindcss": "^3.0.24",
    "uuid": "^8.3.2",
    "web3modal": "^1.9.6"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.0.1",
    "@testing-library/user-event": "^14.1.0",
    "@types/react": "^18.0.3",
    "@types/react-currency-format": "^1.0.0",
    "@types/react-dropzone": "^5.1.0",
    "@types/react-test-renderer": "^18.0.0",
    "@types/request": "^2.48.8",
    "eslint": "^8.13.0",
    "eslint-config-next": "^12.1.5",
    "jest": "^27.5.1",
    "prisma": "^3.12.0",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3",
    "yarn-upgrade-all": "^0.7.1"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  }
