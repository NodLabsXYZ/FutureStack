{
  "name": "basic-next-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "supabase": "yarn && supabase stop && supabase start && npx prisma migrate deploy",
    "stop": "supabase stop",
    "dev": "npx next dev",
    "dev:next_template": "NEXT_TEMPLATE=1 npx next dev --port 3001",
    "build": "npx next build",
    "start": "npx next start",
    "lint": "npx next lint",
    "test": "npx jest --watch",
    "test:next_template": "npx jest --testLocationInResults --forceExit --run-in-band --reporters=\"<rootDir>/__tests__/next_template_reporter.js\"",
    "test:next_template:path": "sh -c 'npx jest ${0} --testLocationInResults --forceExit --runTestsByPath --run-in-band --reporters=\"<rootDir>/__tests__/next_template_reporter.js\"'",
    "test:ci": "npx jest --runInBand",
    "e2e": "npx playwright test __e2e__"
  },
  "dependencies": {
    "@bundlr-network/client": "^0.5.18",
    "@headlessui/react": "^1.5.0",
    "@heroicons/react": "^1.0.6",
    "@prisma/client": "3.9.0",
    "@ramonak/react-progress-bar": "^4.4.0",
    "@stripe/stripe-js": "^1.24.0",
    "@supabase/supabase-auth-helpers": "^1.0.6",
    "@supabase/supabase-js": "^1.29.2",
    "@tailwindcss/typography": "^0.5.2",
    "@walletconnect/web3-provider": "^1.7.1",
    "arweave": "^1.10.23",
    "arweave-nft-uploader": "^1.1.2",
    "autoprefixer": "^10.4.0",
    "ethers": "^5.5.1",
    "formidable": "^2.0.1",
    "next": "12.1.0",
    "postcss": "^8.3.11",
    "react": "17.0.2",
    "react-code-blocks": "^0.0.9-0",
    "react-currency-format": "^1.1.0",
    "react-dom": "17.0.2",
    "react-ellipsis-text": "^1.2.1",
    "react-json-view": "^1.21.3",
    "react-markdown": "^8.0.0",
    "react-spinners": "^0.11.0",
    "react-test-renderer": "^17.0.2",
    "request": "^2.88.2",
    "sharp": "^0.30.2",
    "store2": "^2.13.1",
    "stripe": "^8.207.0",
    "tailwindcss": "^3.0.23",
    "uuid": "^8.3.2",
    "web3modal": "^1.9.5"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@types/react": "^17.0.27",
    "@types/react-currency-format": "^1.0.0",
    "@types/react-dropzone": "^5.1.0",
    "@types/request": "^2.48.8",
    "eslint": "7.32.0",
    "eslint-config-next": "11.1.2",
    "jest": "^27.2.2",
    "pre-commit": "^1.2.2",
    "prisma": "3",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
    "typescript": "^4.3.5"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  }
}
